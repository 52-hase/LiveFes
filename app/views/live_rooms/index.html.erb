<div class="container mt-5">
  <div class="card bg-transparent border-0">
    <div class="list-group list-group-flush custom-room-list">
      <% @live_rooms.each do |live_room| %>
        <a href="<%= live_room_path(live_room) %>" class="list-group-item list-group-item-action d-flex justify-content-center align-items-center" style="color: white; text-align: center;">
          <%= live_room.name %>
        </a>
      <% end %>
      </br></br>
    </div>
  </div>
</div>