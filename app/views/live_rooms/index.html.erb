<div class="container">
<h1 class="text-center my-4 text-white">ライブルーム</h1>
  <div class="card">
    <div class="list-group list-group-flush">
      <% @live_rooms.each do |live_room| %>
        <a href="<%= live_room_path(live_room) %>" class="list-group-item list-group-item-action d-flex justify-content-between align-items-center">
          <%= live_room.name %>
        </a>
      <% end %>
    </div>
  </div>
</div>
